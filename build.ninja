rule Cpp-ar
  description = ar $out
  command = ar -crs $out $in

rule Cpp-so
  description = c++ $out
  command = c++ -shared -o $out $in

rule Cpp-ld
  description = c++ $out
  command = c++ -o $out $in

build $builddir/tmp/lzip.tmp/lzip-$lzip_version/arg_parser.o: Cpp-cc lzip-$lzip_version/arg_parser.cc
  progversion = $lzip_version
build $builddir/tmp/lzip.tmp/lzip-$lzip_version/decoder.o: Cpp-cc lzip-$lzip_version/decoder.cc
  progversion = $lzip_version
