  progversion = $lzip_version
build $builddir/tmp/lzip.tmp/lzip-$lzip_version/main.o: Cpp-cc lzip-$lzip_version/main.cc
  progversion = $lzip_version
build $builddir/lzip: Cpp-ld $builddir/tmp/lzip.tmp/lzip-$lzip_version/arg_parser.o $builddir/tmp/lzip.tmp/lzip-$lzip_version/decoder.o $builddir/tmp/lzip.tmp/lzip-$lzip_version/encoder.o $builddir/tmp/lzip.tmp/lzip-$lzip_version/encoder_base.o $builddir/tmp/lzip.tmp/lzip-$lzip_version/fast_encoder.o $builddir/tmp/lzip.tmp/lzip-$lzip_version/list.o $builddir/tmp/lzip.tmp/lzip-$lzip_version/lzip_index.o $builddir/tmp/lzip.tmp/lzip-$lzip_version/main.o

