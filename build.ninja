  command = zig c++ -target x86_64-windows-gnu -c -MD -MF $depfile -O3 -s -Ilzlib-$lzlib_version -DPROGVERSION='"$progversion"' $in -o $out
  depfile = $out.d

rule zigcpp-windows-x86_64-ar
  description = zig ar $out
  command = zig ar -crs $out $in

rule zigcpp-windows-x86_64-so
  description = zig c++ -target x86_64-windows-gnu $out
  command = zig c++ -target x86_64-windows-gnu -shared -o $out $in

rule zigcpp-windows-x86_64-ld
  description = zig c++ -target x86_64-windows-gnu $out
  command = zig c++ -target x86_64-windows-gnu -o $out $in

build $builddir/tmp/windows-x86_64/lzip.exe.tmp/lzip-$lzip_version/arg_parser.o: zigcpp-windows-x86_64-cc lzip-$lzip_version/arg_parser.cc
